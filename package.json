{
  "name": "react-project-template",
  "author": {
    "name": "qDanik",
    "email": "qdanik@yandex.ru",
    "url": "https://vk.com/qdanik"
  },
  "version": "3.0.0",
  "main": "src/presentation/web/index.tsx",
  "scripts": {
    "preview": "vite --config ./configs/vite/config.ts --port 3030 --mode=web",
    "start": "vite --config ./configs/vite/config.ts --host --mode=web",
    "build:web": "vite --config ./configs/vite/config.ts build --mode=web",
    "start:mobile": "vite --config ./configs/vite/config.ts --host --mode=mobile",
    "build:mobile": "vite --config ./configs/vite/config.ts build --mode=mobile",
    "build": "npm run build:web && npm run build:mobile",
    "lint": "npm run eslint && npm run tslint",
    "eslint": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\" --max-warnings 0",
    "eslint:fix": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\" --max-warnings 0 --fix",
    "eslint:perf": "TIMING=1 npm run eslint",
    "tslint": "tsc --pretty --noEmit",
    "jest": "jest --config ./configs/jest/config.ts",
    "test": "npm run jest --verbose --runInBand",
    "test:ci": "npm run test --no-cache --coverage",
    "postinstall": "husky install ./configs/husky && chmod ug+x ./configs/husky/*",
    "pre-commit": "npm run lint",
    "pre-push": "npm run build && npm run test --coverage",
    "commitlint": "commitlint -e"
  },
  "dependencies": {
    "axios": "0.27.2",
    "date-fns": "2.29.1",
    "i18next": "21.8.16",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-resources-to-backend": "1.0.0",
    "inversify": "6.0.1",
    "lodash": "^4.17.21",
    "mobx": "6.6.1",
    "mobx-react": "7.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.34.0",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "reflect-metadata": "0.1.13",
    "styled-components": "5.3.5",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@svgr/core": "6.3.1",
    "@svgr/plugin-jsx": "6.3.1",
    "@svgr/plugin-svgo": "6.3.1",
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "@vitejs/plugin-react": "2.0.0",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "eslint": "8.20.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-path": "1.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "jest": "28.1.3",
    "less": "4.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "vite": "3.0.4",
    "vite-plugin-cdn-import": "0.3.5",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-html": "3.2.0",
    "vite-tsconfig-paths": "3.5.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "babel": {
    "extends": "./configs/.babelrc"
  },
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc"
    ]
  }
}